<Window x:Class="ComposibilityToolProject.ProgressBarInderminate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ComposibilityToolProject"
        mc:Ignorable="d"
        Title="progressbar" Height="40" Width="100">
    <Grid Name="popupGrid" Visibility="Visible">
        <ProgressBar Minimum="0" Maximum="100" Name="pbStatus" IsIndeterminate="True" />
    </Grid>
</Window>
